<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projects.sharvani.activitylevelsp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ActivityLevelPreferences"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SharedPreferencesExample"
            android:label="@string/title_activity_shared_preferences_example" >
        </activity>
        <activity
            android:name=".RetreiveSharedPreferences"
            android:label="@string/title_activity_retreive_shared_preferences" >
        </activity>
    </application>

</manifest>
